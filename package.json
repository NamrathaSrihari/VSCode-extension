{
  "name": "gitCommitFun",
  "displayName": "gitCommitFun",
  "description": "Fun animations for git commit",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",   
    "onCommand:gitCommitFun.helloWorld"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "gitCommitFun.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
